{
  "name": "api",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:db\"",
    "start": "node dist/server.js",
    "build": "tsup src/server.ts --format cjs --target node22",
    "dev:server": "tsx watch --env-file=.env src/server.ts",
    "dev:db": "npx prisma studio",
    "test": "vitest run --project unit",
    "test:watch": "vitest run --watch --project unit",
    "test:e2e": "vitest run --project e2e",
    "test:e2e:watch": "vitest --project e2e",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.17.0",
    "@types/nodemailer": "^7.0.3",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.6.0",
    "fastify-type-provider-zod": "^6.0.0",
    "nodemailer": "^7.0.10",
    "prisma": "^6.17.0",
    "supertest": "^7.1.4",
    "task-master-ai": "^0.26.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/node": "^24.0.3",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-tsconfig-paths": "^3.4.1"
  }
}